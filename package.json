{
  "name": "mmm-toast-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./scripts/copy-styles.js demo && ng serve",
    "build": "ng build --configuration production mmm-toast && node ./scripts/copy-styles.js",
    "gh-pages": "ng build --configuration production --output-path docs --base-href /mmm-toast/ && node ./scripts/copy-styles.js demo",
    "test": "ng test mmm-toast --browsers=ChromiumHeadless --watch=true",
    "test-ci": "ng test mmm-toast --browsers=ChromiumHeadless --watch=false",
    "lint": "ng lint mmm-toast",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/router": "^14.2.8",
    "bootstrap": "^5.2.2",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.7",
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.7",
    "@angular/compiler-cli": "^14.2.8",
    "@types/jasmine": "^3.10.6",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.25",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "fs-extra": "10.1.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
